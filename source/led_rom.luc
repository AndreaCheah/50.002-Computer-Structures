module led_rom (
    input address[10], // 1024 entries in the rom maximum
    output out[5][3]
  ) {
  
  const LED_SEQ = $reverse({15b000, 15b001, 15b000000000001010, 15b000000001010100, 15b000001010100001,
      15b001010100001010, 15b010100001010100, 15b100001010100001, 15b001010100001010, 15b010100001010100,
      15b100001010100001, 15b001010100001010, 15b010100001010100, 15b100001010100001, 15b001010100001010,
      15b010100001010100, 15b100001010100001, 15b001010100001100, 15b010100001100010, 15b100001100010100,
      15b001100010100100, 15b100010100100001, 15b010100100001100, 15b100100001100010, 15b100001100010001,
      15b001100010001010, 15b100010001010001, 15b010001010001100, 15b001010001100001, 15b010001100001010,
      15b001100001010100, 15b001100001010100, 15b100001010100100, 15b001010100100100, 15b010100100100010,
      15b100100100010100, 15b100100010100001, 15b100010100001010, 15b010100001010001, 15b100001010001100,
      15b001010001100001, 15b010001100001010, 15b001100001010001, 15b100001010001100, 15b001010001100001,
      15b010001100001100, 15b001100001100100, 15b100001100100001, 15b001100100001001, 15b100100001001010,
      15b100001001010100, 15b001001010100010, 15b001010100010001, 15b001010100010001, 15b010100010001010,
      15b100010001010001, 15b010001010001100, 15b001010001100100, 15b010001100100010, 15b001100100010001,
      15b100100010001010, 15b100010001010001, 15b010001010001100, 15b001010001100100, 15b010001100100010,
      15b001100100010100, 15b100100010100010, 15b100010100010010, 15b010100010010001, 15b100010010001100});
  
  always {
    out[4] = LED_SEQ[address][2:0]; // top most row in 5 x 3 led 
    out[3] = LED_SEQ[address][5:3];
    out[2] = LED_SEQ[address][8:6];
    out[1] = LED_SEQ[address][11:9];
    out[0] = LED_SEQ[address][14:12]; // bottom most row 5 x 3 led
    
}